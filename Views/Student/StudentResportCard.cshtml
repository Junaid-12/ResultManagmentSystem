@model List<ResultManagmentSystem.Models.Data.ReportCardMo>


<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />



<header>
    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-white bg-dark border-bottom box-shadow mb-3">
        <div class="container-fluid ">
            <a class="navbar-brand text-white" asp-area="" asp-controller="Home" asp-action="Index">ResultManagmentSystem</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                <ul class="navbar-nav flex-grow-1">
                    <li class="nav-item">
                        <a class="nav-link  text-white " asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li class="nav-item">

                        <a class="nav-link text-white " asp-area="" asp-controller="Admin" asp-action="LoginForm">Admin</a>
                    </li>

                </ul>
            </div>
        </div>
    </nav>
</header>

 
<div class="container  d-flex justify-content-center align-content-center mt-xl-5 ">
    <form method="post" asp-action="StudentResportCard">
        <div class="row col-12 text-black text-lg-start  mb-3">
            <label class="form-label" for="rollno"> Please Enter your  Roll No!!</label>
            <input type="text" class="form-control" placeholder="Roll-No" id="rollno" name="rollno" required />
        </div>
      
        <div class="row d-inline col-12">
            <button type="submit" class="btn btn-light btn-outline-success col-3 ">Submit</button>
            <button type="reset" class=" btn btn btn-danger col-3 ">Reset</button>
        </div>
    </form>

</div>

<link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" rel="stylesheet" />
<!-- Toastr JS -->

<script src="~/lib/jquery/dist/jquery.min.js"></script>

@Html.Partial("_ToastrNotification");
