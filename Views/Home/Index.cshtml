<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>


@{
    ViewData["Title"] = "Home Page";
}
<header>
    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-white bg-dark border-bottom box-shadow mb-1">
        <div class="container-fluid ">
            <a class="navbar-brand text-white" asp-area="" asp-controller="Home" asp-action="Index">ResultManagmentSystem</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                <ul class="navbar-nav flex-grow-1">
                    <li class="nav-item">
                        <a class="nav-link  text-white " asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li class="nav-item">

                        <a class="nav-link text-white " asp-area="" asp-controller="Admin" asp-action="LoginForm">Admin</a>
                    </li>
                    <li class="nav-item">

                        <a class="nav-link text-white" asp-area="" asp-controller="Student" asp-action="StudentResportCard">Result</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
</header>

<div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="carousel" data-bs-interval="1500">
    
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/Img/pexels-droosmo-2982449.jpg" class="d-block w-100 h-75" alt="Slide 1">
        </div>
        <div class="carousel-item">
            <img src="~/Img/pexels-caleboquendo-3042432.jpg" class="d-block w-100 h-75" alt="Slide 2">
        </div>
        <div class="carousel-item">
            <img src="~/Img/pexels-element5-1370296.jpg" class="d-block w-100 h-75" alt="Slide 3">
        </div>
        <div class="carousel-item">
            <img src="~/Img/pexels-pixabay-159213.jpg" class="d-block w-100 h-75" alt="Slide 3">
        </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>

    <footer>

    <div class="container-fluid pt-4 ">
    
            <div class=" d-flex" style="margin-left:50px;">
            <a class="text-decoration-none " asp-action="Index" asp-controller="Home">Home</a>

            <a class="text-decoration-none" asp-action="AboutUS" style="margin-left:15px;"> About US</a>
            <p style="margin-left:15px;">@@All Rights Reserved.! @DateTime.Now.Year  </p>
              </div>
            
           
        </div>
    </footer>